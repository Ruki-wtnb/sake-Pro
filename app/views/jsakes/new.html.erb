<div class="container jsake-wrapper">
  <div class="row">
    <div class="col-md-offset-4 col-md-4 col-xs-offset-1 col-xs-10">
      <h2 class="jsake-title"><%= @title %></h2>
      <%= form_for @jsake do |f| %>
      <%= render partial: 'jsakes/form' , locals: {f: f} %>
        
      <div class="form-group">
        <%= f.submit "登録", class: 'btn btn-primary form-control' %>
      </div>
      <% end %>
            
    </div>      
  </div>
</div>