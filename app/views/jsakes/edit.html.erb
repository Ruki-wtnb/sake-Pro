<div class="jsake-wrapper">
  <div class="container">
    <div class="row">
      <div class="col-md-offset-4 col-md-4 col-xs-offset-4 col-xs-4">
        <h2 class="jsake-title"><%= @title %></h2>
        <%= form_for @jsake, :url => {action: "update" } do |f| %>
        <%= render partial: 'jsakes/form', locals: {f: f} %>
        
        <div class="form-group">
          <%= f.submit "修正", class: 'btn btn-success form-control' %>
        </div>
        
        <div class='form-group'>
          <% f.hidden_field :id, value: @update_id %>
        </div>
        <% end %>
        
      </div>
    </div>
  </div>
</div>
